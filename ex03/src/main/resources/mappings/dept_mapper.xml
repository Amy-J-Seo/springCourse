<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bambbang.app.emp.mapper.DepartmentsMapper">
	<select id="getDeptList" resultType="com.bambbang.app.emp.domain.DepartmentsVO">
		SELECT 
				DEPARTMENT_ID,
				DEPARTMENT_NAME,
				MANAGER_ID,
				LOCATION_ID
			FROM DEPARTMENTS
			ORDER BY DEPARTMENT_ID
	</select>



</mapper>