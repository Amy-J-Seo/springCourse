<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bambbang.app.emp.mapper.JobsMapper">
	<select id="getJobList" resultType="com.bambbang.app.emp.domain.JobsVO">
		SELECT 
				JOB_ID,
				JOB_TITLE,
				MIN_SALARY,
				MAX_SALARY
			FROM JOBS
			ORDER BY JOB_TITLE
	</select>



</mapper>